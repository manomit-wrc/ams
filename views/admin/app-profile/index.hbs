<section class="content-header">
      <h1>
        App Profile List
      </h1>
      <ol class="breadcrumb">
        <li><a href="/admin/dashboard"><i class="fa fa-dashboard"></i> Home</a></li>

        <li class="active">App Profile List</li>
      </ol>
    </section>

    <!-- Main content -->
    <section class="content">
      <div class="row">
        <div class="col-xs-12">

          <div class="box">
            <div class="box-header">

            </div>
            <!-- /.box-header -->
            <div class="box-body">
              {{#if succ_add_msg}}
                <div class="alert alert-success" id="result77">
                {{succ_add_msg}}
                </div>
              {{/if}}
              <table id="example1" class="table table-bordered table-striped">
                <thead>
                <tr>
                  <th>User Name</th>
                  <th>Base Location</th>
                  <th>Current Location</th>
                  <th>Base Session Ref</th>
                  <th>Current Session Ref</th>
                  <th>Last Device Type</th>
                  <th>Last IP Address</th>
                  <th>Last Device Ref</th>
                  <th>Last Accessed Time</th>
                  <th>Remark</th>
                </tr>
                </thead>
                <tbody>
                    {{#each appProfile}}
                    <tr>
                      <td>{{this.admin.first_name}} {{this.admin.last_name}}</td>
                      <td>{{this.base_location}}
                      </td>
                      <td>{{this.current_location}}</td>
                      <td>{{this.base_session_ref}}</td>
                      <td>{{this.current_session_ref}}</td>
                      <td>{{this.last_device_type}}</td>
                      <td>{{this.last_ip_address}}</td>
                      <td>{{this.last_device_ref}}</td>
                      <td>{{this.last_accessed_time}}</td>
                      <td>{{this.remarks}}</td>
                    </tr>
                    {{/each}}
                </tbody>

              </table>
            </div>
            <!-- /.box-body -->
          </div>
          <!-- /.box -->
        </div>
        <!-- /.col -->
      </div>
      <!-- /.row -->
    </section>
