<section class="content-header">
      <h1>
        Budget Code Type List
      </h1>
      <ol class="breadcrumb">
        <li><a href="/admin/dashboard"><i class="fa fa-dashboard"></i> Home</a></li>

        <li class="active">Budget Code Type List</li>
      </ol>
    </section>

    <!-- Main content -->
    <section class="content">
      <div class="row">
        <div class="col-xs-12">

          <div class="box">
            <div class="box-header">
              <h3 class="box-title">Budget Code Type List</h3>
            </div>
            <!-- /.box-header -->
            <div class="topbtn"><a href="/admin/budgetcodetype/add"><button type="button" class="btn bg-purple btn-rightad">ADD</button></a></div>
            <div class="box-body">
              {{#if succ_add_msg}}
                <div class="alert alert-success" id="result77">
                {{succ_add_msg}}
                </div>
              {{/if}}
              <table id="example1" class="table table-bordered table-striped">
                <thead>
                <tr>
                  <th>Budget Code</th>
                  <th>Budget Code Type</th>
                  <th>Remarks</th>
                  <th>Action</th>
                </tr>
                </thead>
                <tbody>
                    {{#each budgetcodetype}}
                     <tr>
                      <td>{{this.code}}</td>
                      <td>{{this.budget_code_type}}</td>
                      <td>{{this.remarks}}
                      </td>
                      <td><a href="/admin/budgetcodetype/edit/{{this.id}}" >Edit</a>&nbsp;|&nbsp;<a href="/admin/budgetcodetype/delete/{{this.id}}" onclick="return confirm('Are you sure?')">Delete</a></td>
                    </tr>
                     {{/each}}
                </tbody>

              </table>
            </div>
            <!-- /.box-body -->
          </div>
          <!-- /.box -->
        </div>
        <!-- /.col -->
      </div>
      <!-- /.row -->
    </section>
