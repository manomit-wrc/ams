<section class="content-header">
      <h1>
        Firm Location List
      </h1>
      <ol class="breadcrumb">
        <li><a href="/admin/dashboard"><i class="fa fa-dashboard"></i> Home</a></li>
        
        <a href="/admin/firm-location"><button type="button" class="btn bg-purple btn-rightad" style="padding: 2px 5px 2px 5px;margin: -1px 2px 2px 8px;">Back</button></a>
      </ol>
    </section>

    <!-- Main content -->
    <section class="content">
      <div class="row">
        <div class="col-xs-12">
                             
          <div class="box">
            <div class="box-header">
              <h3 class="box-title">Firm Location List</h3>
            </div>
            <!-- /.box-header -->
            <div class="topbtn"><a href="/admin/firm-location/add"><button type="button" class="btn bg-purple btn-rightad">ADD</button></a></div>
            <div class="box-body">
              {{#if succ_add_msg}}
                <div class="alert alert-success" id="result77">
                {{succ_add_msg}}
                </div>
              {{/if}}
              <table id="example1" class="table table-bordered table-striped">
                <thead>
                <tr>
                  <th>SL#</th>
                  <th>Address Type</th>
                  <th>Line-1</th>
                  <th>Email</th>
                  <th>Phone</th>
                  <th>Action</th>
                </tr>
                </thead>
                <tbody>
                    {{#each result}}
                     <tr>
                      <td>{{inc @index}}</td>
                      <td>{{this.firm_address_type}}</td>
                      <td>{{this.address_line1}}</td>
                      <td>{{this.email}}</td>
                      <td>{{this.mobile}}</td>
                      <td><a href="/admin/firm-location/edit/{{this.id}}" >Edit</a>&nbsp;|&nbsp;<a href="/admin/firm-location/delete/{{this.id}}" onclick="return confirm('Are you sure?')">Delete</a></td>
                    </tr>
                     {{/each}}               
                </tbody>
                
              </table>
            </div>
            <!-- /.box-body -->
          </div>
          <!-- /.box -->
        </div>
        <!-- /.col -->
      </div>
      <!-- /.row -->
    </section>