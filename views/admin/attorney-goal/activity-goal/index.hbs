<section class="content-header">
      <h1>
        Activity Goal List
      </h1>
      <ol class="breadcrumb">
        <li><a href="/admin/dashboard"><i class="fa fa-dashboard"></i> Home</a></li>

        <a href="/admin/attorney-goal"><button type="button" class="btn bg-purple btn-rightad" style="padding: 2px 5px 2px 5px;margin: -1px 2px 2px 8px;">Back</button></a>
      </ol>
    </section>

    <!-- Main content -->
    <section class="content">
      <div class="row">
        <div class="col-xs-12">

          <div class="box">
            <div class="box-header">
            </div>
            <!-- /.box-header -->
            <div class="topbtn">
              <a href="/admin/attorney-goal/activity-goal/add"><button type="button" class="btn bg-purple btn-rightad">ADD</button></a>
            </div>
            <div class="box-body">
              {{#if succ_add_msg}}
                <div class="alert alert-success" id="result77">
                {{succ_add_msg}}
                </div>
              {{/if}}
              <table id="example1" class="table table-bordered table-striped">
                <thead>
                <tr>
                  <th>Attorney Name</th>
                  <th>Activity Goals</th>
                  <th>Remarks</th>
                </tr>
                </thead>
                <tbody>
                    {{#each result}}
                     <tr>
                      <td>{{this.admin.first_name}}&nbsp;{{this.admin.last_name}}</td>
                      <td>{{this.activity_goal}}</td>                 
                      <td>{{this.remarks}}</td>
                     </tr>
                    {{/each}}
                </tbody>

              </table>
            </div>
            <!-- /.box-body -->
          </div>
          <!-- /.box -->
        </div>
        <!-- /.col -->
      </div>
      <!-- /.row -->
    </section>
