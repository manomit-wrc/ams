<section class="content-header">
      <h1>
        All Competitor Rank List
      </h1>
      <ol class="breadcrumb">
        <li><a href="/admin/dashboard"><i class="fa fa-dashboard"></i> Home</a></li>

        <li class="active">All Competitor Rank List</li>
      </ol>
    </section>

    <!-- Main content -->
    <section class="content">
      <div class="row">
        <div class="col-xs-12">

          <div class="box">
            <div class="box-header">
            </div>
            <!-- /.box-header -->
            <div class="box-body">
              {{#if succ_add_msg}}
                <div class="alert alert-success" id="result77">
                {{succ_add_msg}}
                </div>
              {{/if}}
              <table id="example1" class="table table-bordered table-striped">
                <thead>
                <tr>
                  <th>Sl No.</th>
                  <th>Competitor Code</th>
                  <th>Description</th>
                  <th>Remarks</th>
                </tr>
                </thead>
                <tbody>
                    {{#each result}}
                     <tr>
                        <td>{{this.id}}</td>                 
                        <td>{{this.code}}</td>
                        <td>{{this.short_description}}</td>
                        <td>{{this.remarks}}</td>
                     </tr>
                    {{/each}}
                </tbody>

              </table>
            </div>
            <!-- /.box-body -->
          </div>
          <!-- /.box -->
        </div>
        <!-- /.col -->
      </div>
      <!-- /.row -->
    </section>
