<section class="content-header">
      <h1>
        Competitor List
      </h1>
      <ol class="breadcrumb">
        <li><a href="/admin/dashboard"><i class="fa fa-dashboard"></i> Home</a></li>

        <li class="active">Competitor List</li>
      </ol>
    </section>

    <!-- Main content -->
    <section class="content">
      <div class="row">
        <div class="col-xs-12">

          <div class="box">
            <div class="box-header">
            </div>
            <!-- /.box-header -->
            <div class="topbtn"><a href="/admin/competitor/add"><button type="button" class="btn bg-purple btn-rightad">ADD</button></a></div>
            <div class="box-body">
              {{#if succ_add_msg}}
                <div class="alert alert-success" id="result77">
                {{succ_add_msg}}
                </div>
              {{/if}}
              <table id="example1" class="table table-bordered table-striped">
                <thead>
                <tr>
                  <th>SL#</th>
                  <th>Competitor Name</th>
                  <th>DOB</th>
                  <th>Competitor Ranking</th>
                  <th>Bar Date</th>
                  <th>Bar State</th>
                  <th>Experience</th>
                  <th>Remarks</th>
                  <th>Action</th>
                </tr>
                </thead>
                <tbody>
                    {{#each result}}
                     <tr>
                        <td>{{this.id}}</td>
                        <td>{{this.first_name}}&nbsp;{{this.last_name}}</td>
                        <td>{{this.dob}}</td>
                        <td>{{this.independent}}</td>
                        <td>{{this.bar_date}}</td>                   
                        <td>{{this.bar_belongs_to_state}}</td>
                        <td>{{this.experience}}</td>
                        <td>{{this.remarks}}</td>
                        <td><a href="/admin/competitor/edit/{{this.id}}" >Edit</a>&nbsp;|&nbsp;<a href="/admin/competitor/delete/{{this.id}}" onclick="return confirm('Are you sure to delete?')">Delete</a></td>
                     </tr>
                    {{/each}}
                </tbody>

              </table>
            </div>
            <!-- /.box-body -->
          </div>
          <!-- /.box -->
        </div>
        <!-- /.col -->
      </div>
      <!-- /.row -->
    </section>
