<section class="content-header">
      <h1>
        Code Master List
      </h1>
      <ol class="breadcrumb">
        <li><a href="/admin/dashboard"><i class="fa fa-dashboard"></i> Home</a></li>

        <li class="active">Code Master List</li>
      </ol>
    </section>

    <!-- Main content -->
    <section class="content">
      <div class="row">
        <div class="col-xs-12">
          {{#if message}}<div class="alert alert-success" id="result77" style="display:none;">{{message}}</div>{{/if}}

          <div class="box">
            <div class="box-header">
              <h3 class="box-title">Code Master List</h3>
            </div>
            <!-- /.box-header -->
            <div class="topbtn"><a href="/admin/codemaster/add"><button type="button" class="btn bg-purple btn-rightad">ADD</button></a></div>
            <div class="box-body">
              <table id="example1" class="table table-bordered table-striped">
                <thead>
                <tr>
                  <th>Category</th>
                  <th>Code</th>
                  <th>Short Description</th>
                  <th>Long Description</th>
                  <th>Remarks</th>
                  <th>Action</th>
                </tr>
                </thead>
                <tbody>
                    {{#each codemaster}}
                     <tr>
                      <td>{{this.Codecategory.categoryname}}</td>
                      <td>{{this.code}}</td>
                      <td>{{this.shortdescription}}</td>
                      <td>{{this.longdescription}}</td>
                      <td>{{this.remarks}}</td>
                      <td><a href="/admin/codemaster/edit/{{this.id}}" >Edit</a>&nbsp;|&nbsp;<a href="/admin/codemaster/delete/{{this.id}}" onclick="return confirm('Are you sure?')">Delete</a></td>
                    </tr>
                     {{/each}}
                </tbody>

              </table>
            </div>
            <!-- /.box-body -->
          </div>
          <!-- /.box -->
        </div>
        <!-- /.col -->
      </div>
      <!-- /.row -->
    </section>
